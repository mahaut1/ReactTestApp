{
  "name": "@mahautw/react_test_app",
  "version": "0.1.13",
  "private": false,
  "homepage": "https://mahaut1.github.io/ReactTestApp/",
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "axios": "^1.9.0",
    "cra-template": "1.2.0",
    "faker": "^6.6.6",
    "react": "^19.0.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "jsdoc": "jsdoc -R ./README.md -c ./jsdoc.config.json -r -d ./public/docs",
    "build-npm": "NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src --out-dir dist --copy-files",
    "cypress": "cypress open"
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!axios)",
      "/dist/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/src/mocks/styleMock.js"
    },
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/reportWebVitals.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "cypress": "^14.4.1",
    "dotenv": "^16.5.0",
    "gh-pages": "^6.3.0",
    "jsdoc": "^4.0.4"
  },
  "nyc": {
  "exclude": [
    "src/index.js",
    "src/reportWebVitals.js"
  ]
}


}

